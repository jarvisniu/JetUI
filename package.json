{
  "name": "jet-ui",
  "version": "0.1.6-wip",
  "description": "JetBrains style Vue UI components",
  "main": "dist/jet-ui.min.js",
  "module": "dist/jet-ui.esm.js",
  "scripts": {
    "dev": "nuxt",
    "dist": "rm -rf dist && rollup -c",
    "build": "nuxt build",
    "upload": "upload-oss --output-dir docs --oss-base-dir jet-ui --clean",
    "index": "scp docs/index.html root@aliyun:/www/niujunwei.com/jet-ui",
    "deploy": "npm run build && npm run upload && npm run index"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jarvisniu/jet-ui.git"
  },
  "keywords": [
    "ui",
    "widget",
    "vue"
  ],
  "author": "Jarvis Niu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jarvisniu/jet-ui/issues"
  },
  "homepage": "https://github.com/jarvisniu/jet-ui#readme",
  "devDependencies": {
    "@jarvisniu/upload-oss": "^1.1.1",
    "lodash.findindex": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "modern-normalize": "^0.6.0",
    "nuxt": "^2.14.4",
    "rollup": "^1.27.13",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-stylus-css-modules": "^1.5.0",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "5.1.1",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2"
  },
  "files": [
    "dist/**/*"
  ]
}
